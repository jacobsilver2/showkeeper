<h2> Shows at <%= current_user.venue%> </h2>

<% @shows.each do |show| %>
  <div>
    <br>
    <p><%= show.date %></p>
    <p><%= show.doors_at %></p>
    <p><a href="<%=show.headliner_url%>"><%= show.headliner %></a></p>
    <p><%= show.support %></p>
    <p><%= show.blurb %></p>
    <br>
    <a href="/shows/<%=show.id%>/edit">Edit Show </a>
    <form method="post" action="/shows/<%=show.id%>/delete">
    <input id="hidden" type="hidden" name="_method" value="DELETE">
    <input type="submit" value="Delete Show">
    <br>
    </form>
  </div>
<% end %>